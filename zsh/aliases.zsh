# Sublime

alias sbl="'/Applications/Sublime Text.app/Contents/SharedSupport/bin/subl'"

alias reload!='. ~/.zshrc'
alias zshrc='sbl ~/.zshrc'
alias dotfiles='cd ~/.dotfiles'

# iOS
alias blastDerivedData='rm -rf ~/Library/Developer/Xcode/DerivedData/*'

alias be='bundle exec'

alias bef='bundle exec fastlane'
alias rgdev='bef register_devices_prompt'
alias rgfile='bef register_devices_file'